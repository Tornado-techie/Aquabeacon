# AquaBeacon Client Environment Variables
# Copy this file to .env and replace with your actual values

# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api
VITE_API_URL=http://localhost:5000/api

# App Configuration
VITE_APP_NAME=AquaBeacon
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PUSH_NOTIFICATIONS=false

# External Services
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Environment
VITE_ENVIRONMENT=development

# Monitoring & Error Tracking
# Sentry for error tracking and performance monitoring
VITE_SENTRY_DSN=your_sentry_client_dsn_here
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
VITE_SENTRY_REPLAY_SAMPLE_RATE=0.1
VITE_SENTRY_RELEASE=aquabeacon-client@1.0.0
VITE_ENABLE_SENTRY=false